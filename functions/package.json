{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "dev": "yarn serve & yarn parcelWatch",
    "serve": "firebase serve",
    "parcelWatch": "parcel watch app/clientIndex.jsx --out-dir ../public/bundle --public-url ./",
    
    "deploy": "yarn parcelBuild; firebase use staging; firebase deploy",
    "parcelBuild": "parcel build app/clientIndex.jsx --out-dir ../public/bundle --public-url ./"
  },
  "engines": {
    "node": "10"
  },
  "dependencies": {
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "google-spreadsheet": "^3.0.10",
    "ignore-styles": "^5.0.1",
    "markdown-it": "^10.0.0",
    "moment": "^2.24.0",
    "parcel-bundler": "^1.12.4",
    "prop-types": "^15.7.2",
    "react": ">=15",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "slugify": "^1.4.0",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-tools": "^8.0.1"
  },
  "private": true
}
